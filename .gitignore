# ===============================
# PYTHON / FLASK .gitignore
# ===============================

# Byte-compiled / optimized / DLL files
__pycache__/
*.py[cod]
*$py.class

# Virtual environments
venv/
.env/
.env.bak/
env/
.venv/
pip-wheel-metadata/
pip-log.txt

# Flask / runtime files
instance/
*.db
*.sqlite3
*.sqlite
*.log
*.pid
*.swp
*.swo

# Unit test / coverage reports
htmlcov/
.tox/
.nox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*.cover
*.py,cover
*.hypothesis/

# PyInstaller
# (usually created by a Python script to package the app)
build/
dist/
*.spec

# Jupyter Notebooks checkpoints
.ipynb_checkpoints/

# pyenv
.python-version

# dotenv / credentials
*.env
*.flaskenv
config.py.local
settings_local.py

# IDEs / Editors
.vscode/
.idea/
*.iml
*.sublime-workspace
*.sublime-project

# OS-specific
.DS_Store
Thumbs.db

# Docker / Containers (if you add later)
docker-compose.override.yml
*.bak

# Documentation / cache
docs/_build/
site/

# Temp or generated files
tmp/
temp/
*.bak
*.tmp
*.orig
*.old

# ===============================
# NODE.JS / EXPRESS / CALLBACK SERVER
# ===============================

# Dependency directories
CALLBACK_NODE_JS/node_modules/

# Logs
CALLBACK_NODE_JS/npm-debug.log*
CALLBACK_NODE_JS/yarn-debug.log*
CALLBACK_NODE_JS/yarn-error.log*
CALLBACK_NODE_JS/pnpm-debug.log*

# Environment variables
CALLBACK_NODE_JS/.env
CALLBACK_NODE_JS/.env.local
CALLBACK_NODE_JS/.env.*.local

# Build output
CALLBACK_NODE_JS/dist/
CALLBACK_NODE_JS/build/
CALLBACK_NODE_JS/.next/
CALLBACK_NODE_JS/out/

# IDE/editor artifacts (just in case)
CALLBACK_NODE_JS/.vscode/
CALLBACK_NODE_JS/.idea/
CALLBACK_NODE_JS/*.log
